{
  "name": "Remove chinese",
  "version": "1.0",
  "description": "Remove china repo from github.",
  "manifest_version": 2,
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "content_scripts": [
    {
      "run_at": "document_idle",
      "matches": ["https://github.com/*"],
      "js": [
        "util.js",
        "constants.js",
        "content_main.js",
        "content_discover.js"
      ],
      "css": ["content.css"]
    }
  ],
  "permissions": ["tabs"]
}
